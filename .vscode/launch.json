  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'ikari'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=ikari"
        ],
        "filter": {
          "name": "ikari",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "env": {
        "RUST_LOG": "wgpu_core::device=debug",
      },
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'example_game'",
      "cargo": {
        "args": [
          "build",
          "--release",
